# Global properties
command=$XDG_CONFIG_HOME/i3blocks/$BLOCK_NAME
separator=true
separator_block_width=15

[wifi]
interval=5

[brightness]
command=blind --block
interval=once
signal=2

[volume]
command=deaf --block
interval=once
signal=1

[battery]
interval=30

[battery BAT1]
interval=30

[time]
command=date '+%d/%m  %H:%M'
interval=1
